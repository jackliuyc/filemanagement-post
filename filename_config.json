{
    "BIO": {
        "segments": [
            {
                "name": "study",
                "label": "Study",
                "type": "text",
                "default": "BIO",
                "validation": "^BIO$",
                "editable": false
            },
            {
                "name": "phase",
                "label": "Visit",
                "type": "combo",
                "options": [
                    "",
                    "v1",
                    "v2",
                    "v3",
                    "v4",
                    "v5",
                    "v6",
                    "v7",
                    "v8",
                    "v9",
                    "v10",
                    "v11",
                    "v12",
                    "v13",
                    "v14",
                    "v15",
                    "v16",
                    "v17",
                    "v18",
                    "v19",
                    "v20"
                ],
                "default": "",
                "validation": "^(v\\d+)?$",
                "error_message": "Phase must be empty or v1, v2, v3, etc."
            },
            {
                "name": "day",
                "label": "Day",
                "type": "combo",
                "default": "",
                "options": [
                    "",
                    "d1",
                    "d2",
                    "d3",
                    "d4",
                    "d5",
                    "d6",
                    "d7",
                    "d8",
                    "d9",
                    "d10",
                    "d11",
                    "d12",
                    "d13",
                    "d14",
                    "d15",
                    "d16",
                    "d17",
                    "d18",
                    "d19",
                    "d20"
                ],
                "validation": "^(d\\d+)?$",
                "error_message": "Day must be empty or d1, d2, d3, etc.",
                "no_leading_underscore": true
            },
            {
                "name": "file_type",
                "label": "Paradigm",
                "type": "combo",
                "default": "",
                "options": [
                    "",
                    "rest",
                    "chirp",
                    "ssct",
                    "gapassr",
                    "rleeg",
                    "resteyesclosed",
                    "talk",
                    "listen",
                    "vdaudio",
                    "vdnoaudio",
                    "other"
                ],
                "validation": "^[A-Za-z]+$",
                "error_message": "File type must be alphabetic"
            },
            {
                "name": "subject_id",
                "label": "Subject ID",
                "type": "text",
                "validation": "^\\d{5}$",
                "error_message": "Subject ID must be 5 digits"
            },
            {
                "name": "subject_initials",
                "label": "Subject Initials",
                "type": "text",
                "validation": "^[A-Z]{2,3}$",
                "error_message": "Subject Initials must be 2 uppercase letters"
            },
            {
                "name": "date",
                "label": "Date",
                "type": "date",
                "validation": "^\\d{2}-\\d{2}-\\d{4}$",
                "error_message": "Date must be in MM-DD-YYYY format",
                "no_leading_underscore": true
            },
            {
                "name": "capsize",
                "label": "Capsize",
                "type": "combo",
                "options": [
                    "",
                    "adult",
                    "infant"
                ],
                "default": "",
                "validation": "^adult|infant$",
                "error_message": "Capsize must be adult or infant"
            },
            {
                "name": "audio_source",
                "label": "Audio Source",
                "type": "combo",
                "options": [
                    "",
                    "headphones",
                    "speakers"
                ],
                "default": "",
                "validation": "^headphones|speakers$",
                "error_message": "Audio source must be headphones or speaker"
            }
        ],
        "optional_suffixes": [
        ]
    },
